{
  "name": "dwnload/wp-rest-api-object-cache",
  "description": "Enable object caching for WordPress' REST API. Aids in increased response times of your applications endpoints.",
  "type": "wordpress-plugin",
  "version": "0.2",
  "license": "MIT",
  "authors": [
    {
      "name": "Austin Passy",
      "email": "thefrosty@users.noreply.github.com",
      "homepage": "https://austin.passy.co",
      "role": "Developer"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "thefrosty/wp-utilities": "^0.2.2",
    "php": ">=7.0.4"
  },
  "require-dev": {
    "10up/wp_mock": "dev-dev",
    "phpunit/phpunit": "6.*",
    "wp-coding-standards/wpcs": "^0.13.1"
  },
  "scripts": {
    "post-install-cmd": [
      "@phpcs"
    ],
    "post-update-cmd": [
      "@phpcs"
    ],
    "phpcs": "\"vendor/bin/phpcs\" --config-set installed_paths vendor/wp-coding-standards/wpcs"
  },
  "autoload": {
    "psr-4": {
      "Dwnload\\WpRestApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dwnload\\WpRestApi\\Tests\\": "tests"
    }
  }
}
